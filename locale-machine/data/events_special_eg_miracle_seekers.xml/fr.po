"data/events_special_eg_miracle_seekers.xml$//event[@name=\"EG_QUEST_HORN_OF_GOD_EXPLORING_3\"]/choice[1]/event/choice[4]/event/choice/text"
msgstr "Courez vers votre vaisseau !"

#: src-en/data/events_special_eg_miracle_seekers.xml:2822
msgid ""
"data/events_special_eg_miracle_seekers.xml$//event[@name=\"EG_QUEST_HORN_OF_GOD_EXPLORING_3\"]/choice[@req=\"termite_doors_unlocked\""
