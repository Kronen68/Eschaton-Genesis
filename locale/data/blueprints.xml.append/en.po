msgstr "Lower termite"

#: src-en/data/blueprints.xml.append:3941
msgid ""
"data/blueprints.xml.append$//crewBlueprint[@name=\"eg_termite\"]/powerList/power[1]"
msgstr ""
"Passive Ability: Damage and movement speed increase by 1.3x for each other "
"termite in the room"

#: src-en/data/blueprints.xml.append:3942
msgid ""
"data/blueprints.xml.append$//crewBlueprint[@name=\"eg_termite\"]/powerList/power[2]"
msgstr ""
"Max Health is reduced to 90. Occupies half a crew slot on the ship manifest"

#: src-en/data/blueprints.xml.append:3943
msgid ""
"data/blueprints.xml.append$//crewBlueprint[@name=\"eg_termite\"]/powerList/power[3]"
msgstr "Takes 1.3x damage from hazards."

#: src-en/data/blueprints.xml.append:3944
msgid ""
"data/blueprints.xml.append$//crewBlueprint[@name=\"eg_termite\"]/powerList/power[4]"
