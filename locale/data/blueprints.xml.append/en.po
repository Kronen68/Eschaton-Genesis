#: src-en/data/blueprints.xml.append:3929
msgid ""
"data/blueprints.xml.append$//crewBlueprint[@name=\"unique_slavik\"]/powerList/power[2]"
msgstr "Does not occupy a crew slot. Please do not dismiss him"

#: src-en/data/blueprints.xml.append:3934
msgid "data/blueprints.xml.append$//crewBlueprint[@name=\"eg_termite\"]/desc"
msgstr ""
"This species of near-sentient termites appeared in this universe quite "
"recently. Not yet fully settled, they are still searching for a place for "
"their new colony."

#: src-en/data/blueprints.xml.append:3935
msgid "data/blueprints.xml.append$//crewBlueprint[@name=\"eg_termite\"]/title"
msgstr "Lower termite"

#: src-en/data/blueprints.xml.append:3936
