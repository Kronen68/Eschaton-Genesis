msgid ""
"data/events_special_eg_miracle_seekers.xml$//event[@name=\"MIRACLE_SEEKERS_LOBBY\"]/choice[@req=\"seekers_quest_started\"]/event/choice[1]/event/choice[1]/text"
msgstr "Do something onboard the ship."

