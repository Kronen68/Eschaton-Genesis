msgid ""
"data/events_special_miracleseekers.xml$//event[@name=\"EG_QUEST_THANKS_2\"]/choice/event/choice[2]/text"
msgstr "给蛞蝓5单位废料。"

#: src-en/data/events_special_miracleseekers.xml:2284
msgid ""
"data/events_special_miracleseekers.xml$//event[@name=\"EG_QUEST_THANKS_2\"]/choice/event/choice[2]/event/text"
msgstr "“不，这太少了。我知道你还有更多。”"

#: src-en/data/events_special_miracleseekers.xml:2289
msgid ""
"data/events_special_miracleseekers.xml$//event[@name=\"EG_QUEST_THANKS_2\"]/choice/event/choice[2]/event/choice[1]/text"
